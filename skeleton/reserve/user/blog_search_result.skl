<body onload="OnLoad()">
<!--header_area-->
<a name="top"></a>

<!--title_area-->
<table align="center" border="0" cellpadding="0" cellspacing="0" width="760">
<tr>
	<td class="site_title">
##_TITLE_##
##_SUBTITLE_##
	</td>
</tr>
</table>

<!--HEAD OF MAINAREA-->
<table class="tab-design" align="center" border="0" cellpadding="0" cellspacing="0" width="760">
<tr>
	<td>
		<table class="search-result" align="center" border="0" cellspacing="0" cellpadding="0" width="730">
		<tr>
			<td><img src="##_IMAGES_##/transparent.gif" height="7"></td>
		</tr>
		<tr>
			<td align="center" class="visitor-title">ブログ内検索</td>
		</tr>
		<tr valign="top">
			<td>
				<!--result_area-->
				<table border="0" cellspacing="0" cellpadding="0" width="100%">
				<tr>
					<td align="left">
						<form method="post" action="blog_build_html.cgi">
						<input type="hidden" name="BTN_BLOG_SEARCH">
##_HIDDEN_##
						<table border="0" cellspacing="5" cellpadding="0" width="100%">
						<tr>
							<td>
								<input type="text" name="search_string" value="##_STRING_##" size="60" maxlength="256">
								<input type="submit" name="BTN_BLOG_SEARCH" value="検索する">
							</td>
						</tr>
						<tr>
							<td>
								<input type="checkbox" name="search_subject" value="1"##_SUBJECT_##>件名
								<input type="checkbox" name="search_content" value="1"##_CONTENT_##>本文
								<input type="checkbox" name="search_comment" value="1"##_COMMENT_##>コメント
								<input type="checkbox" name="search_trackback" value="1"##_TRACKBACK_##>トラックバック
							</td>
						</tr>
						</table>
						</form>
					</td>
				</tr>
				<tr>
					<td align="left" valign="middle" height="25">検索結果&nbsp;：&nbsp;全 ##_ALL_## 件　##_PAGE_##</td>
				</tr>
##_RESULT_##
##_RESULT_START_##
				<tr>
					<td align="left" valign="middle" height="25">検索結果&nbsp;：&nbsp;全 ##_ALL_## 件　##_PAGE_##</td>
				</tr>
##_RESULT_END_##
				</table>
			</td>
		</tr>
		</table>
		<br>
		<a href="#top"><img src="##_THEME_##/##_BACK_TOP_##" border="0" alt="ページのTOPへ"></a>　
