<body leftmargin="0" topmargin="120" marginwidth="0" marginheight="0">
<div align="center">

<!--メイン-->
<form action="./blog_comment_list.cgi" method="post">
##_HIDDEN_##
<table width="800" align="center" cellpadding="0" cellspacing="0" border="0">
<tr>
	<td style="padding:50px 0 0 0;"></td>
	<td align="center">
		<table width="400" border="0" cellspacing="0" cellpadding="3" align="center" style="border:solid 1px #0A3968;" bgcolor="#ffffff">
		<tr>
			<td><img src="##_IMAGES_##/title_kekka.gif" border="0"></td>
		</tr>
		<tr>
			<td style="padding:10px 0 10px 10px; color:red; font-weight:bold; font-size:13;">下記のコメントを承認しますか?</td>
		</tr>
		<tr>
			<td>
##_MSG_##
				<table width="90%" cellpadding="0" cellspacing="1" border="0" class="a2" align="center" style="border:solid 1px #a5c3ef; padding:4px 0 4px 4px;">
				<tr style=" background-color:#a5c3ef; font-weight:bold;" align="center">
					<td nowrap>投稿者名</td>
					<td nowrap>投稿元記事</td>
					<td nowrap width="140">投稿時間</td>
				</tr>
##_ACCEPT_TITLE_##
				</table>
			</td>
		</tr>
		<tr>
			<td align="center" style="padding:20px 0 0 0;">
				<input type="image" src="##_IMAGES_##/shounin.gif" value="承認する" name="BTN_ACCEPT_COMMENT">
				<input type="image" src="##_IMAGES_##/cancel.gif" value="キャンセル" name="BTN_DISP_COMMENTLIST">
			</td>
		</tr>
		<tr>
			<td height="15"></td>
		</tr>
		</table>
	</td>
	<td width="20"></td>
</tr>
<tr>
	<td colspan="3" height="15"></td>
</tr>
</table>
</form>
</div>

</body>
</html>
